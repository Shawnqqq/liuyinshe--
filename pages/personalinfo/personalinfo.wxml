<view class="page-container">
  <view class="info-title">个人信息</view>
  <view class="data-container">
    <view class="data-title">姓名：</view>
    <view class="data-content">{{userInfo.name}}</view>
  </view>
  <view class="data-container">
    <view class="data-title">联系电话：</view>
    <view class="data-content">{{userInfo.phone}}</view>
  </view>
  <view class="data-container">
    <view class="data-title">性别：</view>
    <view class="data-content">{{userInfo.sex}}</view>
  </view>
  <view class="data-container">
    <view class="data-title">生日：</view>
    <view class="data-content">{{userInfo.birthday}}</view>
  </view>
  <view class="data-container">
    <view class="data-title">紧急联系人：</view>
    <view class="data-content">{{userInfo.sms_name}}</view>
  </view>
  <view class="data-container">
    <view class="data-title">紧急联系电话：</view>
    <view class="data-content">{{userInfo.sms_phone}}</view>
  </view>
  <view class="data-container">
    <view class="data-title">账户余额：</view>
    <view class="data-content">{{userInfo.balance}}</view>
  </view>
  <view class="data-container">
    <view class="data-title">创建时间：</view>
    <view class="data-content">{{userInfo.created_at}}</view>
  </view>
</view>